{
    "name": "kindle_clippings_webapp",
    "version": "0.1.1",
    "private": true,
    "dependencies": {
        "@ant-design/icons": "^4.7.0",
        "@reduxjs/toolkit": "^1.7.2",
        "clipboard-copy": "^4.0.1",
        "cross-env": "^7.0.3",
        "dayjs": "^1.10.7",
        "eslint-plugin-jest": "^26.0.0",
        "fuse.js": "^6.5.3",
        "lodash": "^4.17.21",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-dropzone": "^12.0.0",
        "react-redux": "^7.2.6",
        "react-router-dom": "^6.2.1",
        "react-scripts": "^5.0.1",
        "react-toastify": "^8.1.1",
        "semantic-ui-css": "^2.5.0",
        "semantic-ui-react": "^2.1.4",
        "styled-components": "^5.3.3",
        "uuid": "^8.3.2",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "nf start -p 3000",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom --watchAll=false",
        "eject": "react-scripts eject",
        "electron": "electron .",
        "electron-start": "node src/start-react",
        "react-start": "react-scripts start",
        "pack:windows": "electron-builder -w",
        "dist": "npm run build && build",
        "postinstall": "install-app-deps",
        "test:coverage": "cross-env CI=true npm test -- --env=jsdom --coverage",
        "prepare": "husky install",
        "format": "npx prettier --write .",
        "lint": "npx eslint . --ext .js,.jsx"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/dom": "^9.0.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@trivago/prettier-plugin-sort-imports": "^3.2.0",
        "electron": "^17.0.0",
        "electron-builder": "^22.14.13",
        "electron-is-dev": "^2.0.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "foreman": "^3.0.1",
        "husky": "^7.0.4",
        "lint-staged": "^12.3.3",
        "prettier": "2.5.1"
    },
    "homepage": "./",
    "main": "src/start.js",
    "build": {
        "appId": "com.kindleclippingsapp",
        "win": {
            "icon": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
        },
        "directories": {
            "buildResources": "public"
        }
    },
    "lint-staged": {
        "**/*": [
            "prettier --write --ignore-unknown"
        ]
    }
}
